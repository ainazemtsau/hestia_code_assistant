{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "CSK scope proof",
  "type": "object",
  "required": [
    "task_id",
    "module",
    "ran_utc",
    "slice",
    "overall_ok",
    "changed_files"
  ],
  "properties": {
    "task_id": {
      "type": "string"
    },
    "module": {
      "type": "string"
    },
    "ran_utc": {
      "type": "string"
    },
    "slice": {
      "type": [
        "string",
        "null"
      ]
    },
    "overall_ok": {
      "type": "boolean"
    },
    "changed_files": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "violations": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  }
}