    "task_id",
    "module_id",
    "slice_id",
    "created_at",
    "gates"
  ],
  "properties": {
    "task_id": {
      "type": "string"
    },
    "module_id": {
      "type": "string"
    },
    "slice_id": {
      "type": "string"
    },
    "created_at": {
      "type": "string",
      "format": "date-time"
    },
    "gates": {
      "type": "object"
    },
    "artifacts": {
      "type": "object"
    }
  },
  "additionalProperties": true
}